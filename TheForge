local placeId = game.PlaceId

if placeId == 76558904092080 then
    -- Script for first game
    loadstring(game:HttpGet("https://raw.githubusercontent.com/anurajsaru/RaZui/refs/heads/main/ForgeIsland1"))()

elseif placeId == 129009554587176 then
    -- Script for second game
    loadstring(game:HttpGet("https://raw.githubusercontent.com/anurajsaru/RaZui/refs/heads/main/ForgeIsland2"))()

else
    -- Optional: do nothing / print
    print("This script is not configured for this game. PlaceId:", placeId)
end
